macro=;
 if ( argnumber < 2 ) errmsg Give trees to compare as argument; end
 set 0 collapse ;
 collapse none ;
 tcomp * %1 %2 ;
 tcomp * %2 %1 ;
 set 1 ((tnodes[ntrees]+tnodes[(ntrees-1)])/(tnodes[%1]+tnodes[%2]));
 collapse '0' ;
 set 0 ntrees - 1 ;
 keep '0' ;
quote Robinson-Foulds distance between trees %1-%2 is '/.3(1)' ;
log rflog.log;
quote '/.3(1)' ;
log/;

proc/;

